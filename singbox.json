    {
      "tag": "日本",
      "type": "urltest",
      "use_all_providers": true,
      "includes": "日本|JP|Japan",
      "excludes": "ipv6",
      "url": "https://www.apple.com/library/test/success.html",
      "interval": "1m",
      "tolerance": 100,
