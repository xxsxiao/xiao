{
  "log": {
    "disabled": false,
    "level": "panic",
    "output": "box.log",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
      "external_ui_download_detour": "国外策略",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "path": "cache.db",
      "store_fakeip": true,
      "store_rdrc": true
    },
    "urltest_unified_delay": true
  },
  
  "dns": {
    "servers": [
      {
        "type": "quic",
        "tag": "国内dns",
        "server": "223.5.5.5"
      },
      {
        "type": "fakeip",
        "tag": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "rule_set": ["geosite:cn", "geosite:googlefcm", "geosite:google-cn", "geosite:apple-cn", "geosite:category-games@cn"],
        "action": "route",
        "server": "国内dns",
        "strategy": "ipv4_only"
      },
      {
        "clash_mode": "DIRECT",
        "server": "国内dns",
        "strategy": "ipv4_only"
      },
      {
        "clash_mode": "GLOBAL",
        "server": "fakeip"
      },
      {
        "inbound": ["mixed-in", "tproxy-in", "redirect-in", "tun-in"],
        "server": "fakeip"
      }
    ],
    "strategy": "ipv4_only",
    "disable_cache": false,
    "disable_expire": false,
    "independent_cache": false,
    "reverse_mapping": false
  },
  
  "inbounds": [
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "0.0.0.0",
      "listen_port": 7890
    },
    {
      "type": "tproxy",
      "tag": "tproxy-in",
      "listen": "0.0.0.0",
      "listen_port": 1536
    },
    {
      "type": "redirect",
      "tag": "redirect-in",
      "listen": "0.0.0.0",
      "listen_port": 7891
    },
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "meta",
      "address": ["198.18.0.0/30", "fdfe:dcba:9876::1/126"],
      "mtu": 9000,
      "auto_route": true,
      "auto_redirect": false,
      "strict_route": false,
      "route_address": ["0.0.0.0/1", "128.0.0.0/1", "::/1", "8000::/1"],
      "route_exclude_address": ["192.168.0.0/16", "fc00::/7"],
      "endpoint_independent_nat": false,
      "udp_timeout": "5m",
      "stack": "system"
    }
  ],
  
  "outbounds": [
    {
      "tag": "国外策略",
      "type": "selector",
      "outbounds": ["国外自动选择", "国外手动选择", "香港", "台湾", "新加坡", "日本", "韩国", "美国"],
      "interrupt_exist_connections": true
    },
    {
      "tag": "国内手动选择",
      "type": "selector",
      "use_all_providers": true,
      "includes": "移动|联通|电信",
      "excludes": "ipv6",
      "interrupt_exist_connections": true
    },
    {
      "tag": "国外自动选择",
      "type": "urltest",
      "use_all_providers": true,
      "includes": "香港|HK|hk|Hong Kong|HongKong|hongkong|台湾|TW|Taiwan|新加坡|SG|Singapore|日本|JP|Japan|韩国|KR|Korea|美国|US|United States|UnitedStates",
      "excludes": "ipv6",
      "url": "https://www.apple.com/library/test/success.html",
      "interval": "1m",
      "tolerance": 100,
      "interrupt_exist_connections": true
    },
    {
      "tag": "国外手动选择",
      "type": "selector",
      "use_all_providers": true,
      "includes": "香港|HK|hk|Hong Kong|HongKong|hongkong|台湾|TW|Taiwan|新加坡|SG|Singapore|日本|JP|Japan|韩国|KR|Korea|美国|US|United States|UnitedStates",
      "excludes": "ipv6",
      "interrupt_exist_connections": true
    },
    {
      "tag": "广告拦截",
      "type": "selector",
      "outbounds": ["block", "pass"],
      "interrupt_exist_connections": true
    },
    {
      "tag": "openai",
      "type": "selector",
      "outbounds": ["国外自动选择", "国外手动选择", "香港", "台湾", "新加坡", "日本", "韩国", "美国"],
      "interrupt_exist_connections": true
    },
    {
      "tag": "香港",
      "type": "urltest",
      "use_all_providers": true,
      "includes": "香港|HK|hk|Hong Kong|HongKong|hongkong",
      "excludes": "ipv6",
      "url": "https://www.apple.com/library/test/success.html",
