    },
    {
      "tag": "美国",
      "type": "urltest",
      "use_all_providers": true,
      "includes": "美国|US|United States|UnitedStates",
      "excludes": "ipv6",
      "url": "https://www.apple.com/library/test/success.html",
      "interval": "1m",
      "tolerance": 100,
      "interrupt_exist_connections": true
    },
    {
      "tag": "pass",
      "type": "pass"
    },
    {
      "tag": "block",
      "type": "block"
    },
    {
      "tag": "GLOBAL",
      "type": "selector",
      "use_all_providers": true,
      "interrupt_exist_connections": true
    },
    {
      "tag": "DIRECT",
      "type": "direct"
    }
  ],
  
  "route": {
    "auto_detect_interface": true,
    "default_domain_resolver": {
      "server": "国内dns",
      "strategy": "ipv4_only"
    },
    "rules": [
      {
        "action": "sniff"
