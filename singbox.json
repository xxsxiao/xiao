      "interval": "1m",
      "tolerance": 100,
      "interrupt_exist_connections": true
    },
    {
      "tag": "新加坡",
      "type": "urltest",
      "use_all_providers": true,
      "includes": "新加坡|SG|Singapore",
      "excludes": "ipv6",
      "url": "https://www.apple.com/library/test/success.html",
      "interval": "1m",
      "tolerance": 100,
      "interrupt_exist_connections": true
